{
  "preferences": {
    "menuBarEnabled": false
  },
  "mcpServers": {
    "mysql": {
      "command": "/Library/Frameworks/Python.framework/Versions/3.14/bin/mcp-server-mysql",
      "env": {
        "MYSQL_HOST": "localhost",
        "MYSQL_PORT": "3306",
        "MYSQL_USER": "root",
        "MYSQL_PASSWORD": "Shubham@117",
        "MYSQL_DATABASE": "rahulshettyacademy"
      }
    },
    "playwright": {
      "command": "/usr/local/bin/node",
      "args": [
        "/usr/local/bin/npx",
        "@playwright/mcp@latest"
      ],
      "env": {
        "PATH": "/usr/local/bin:/usr/bin:/bin:/usr/sbin:/sbin",
        "SHELL": "/bin/zsh",
        "HOME": "/Users/shubham_infinity"
      }
    },
    "rest-api": {
      "command": "/usr/local/bin/npx",
      "args": [
        "-y",
        "dkmaker-mcp-rest-api"
      ],
      "env": {
        "REST_BASE_URL": "http://localhost:3000/students",
        "HEADER_Accept": "application/json",
        "PATH": "/usr/local/bin:/usr/bin:/bin:/usr/sbin:/sbin"
      }
    },
    "filesystem": {
      "command": "/usr/local/bin/npx",
      "args": [
        "-y",
        "@modelcontextprotocol/server-filesystem",
        "/Users/shubham_infinity/Desktop/Software_Testing/MCP/Data"
      ],
      "env": {
        "PATH": "/usr/local/bin:/usr/bin:/bin:/usr/sbin:/sbin"
      }
    },
    "excel": {
      "command": "npx",
      "args": [
        "--yes",
        "@negokaz/excel-mcp-server"
      ],
      "env": {
        "EXCEL_MCP_PAGING_CELLS_LIMIT": "4000"
      }
    }
  }
}